{
  "name": "juanyaolin/laravel-api-response-builder",
  "description": "A laravel package for easily build json api response.",
  "homepage": "https://github.com/juanyaolin/laravel-api-response-builder",
  "version": "1.0.1",
  "keywords": [
    "laravel",
    "laravel 10",
    "laravel 11",
    "json",
    "api",
    "response",
    "builder",
    "exception",
    "error"
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Juanyaolin\\ApiResponseBuilder\\": "src/"
    }
  },
  "authors": [
    {
      "name": "juanyaolin",
      "email": "maple96541@gmail.com"
    }
  ],
  "support": {
    "source": "https://github.com/juanyaolin/laravel-api-response-builder/",
    "issues": "https://github.com/juanyaolin/laravel-api-response-builder/issues"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Juanyaolin\\ApiResponseBuilder\\ApiResponseBuilderServiceProvider"
      ]
    }
  },
  "require": {
    "php": "^8.1|^8.2|^8.3",
    "laravel/framework": "^10.0|^11.0"
  }
